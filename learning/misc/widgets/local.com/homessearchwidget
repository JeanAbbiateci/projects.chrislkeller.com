<script type="text/javascript">
function showPremiumListing(category,location,partnerURL)
{document.getElementById('premium-listing').innerHTML='<IFRAME src="http://'+partnerURL+'/widget.aspx?'+'q='+escape(category)+'&p='+escape(partnerURL)+'&z='+escape(location)+'"   frameborder="0" scrolling="no" style="width:100%; height:115px; min-height:10px"></IFRAME>';}
function searchLocal(keyword,location,partnerURL)
{window.location="http://"+partnerURL+"/search.pg?q="+escape(keyword)+"&z="+escape(location);return false;}
</script>

<!-- END SCRIPT -->

